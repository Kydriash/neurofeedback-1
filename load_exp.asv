function eeg = load_exp()
%select folder
folder_name = uigetdir('','Select directory with experiment');
if isempty(folder_name)
    return
end
    
%load exp settings
[protocols, protocols_show_as, durations, channels,settings_file] = GetDataProperties(self.files_pathname,self.fnames);

nfs = NeurofeedbackSession;
nfs.LoadFromFile(strcat(folder_name,'/','Exp_design.xml'));
%load data

%create experiment
eeg = EEGLSL;
%push data


end